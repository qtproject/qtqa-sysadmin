<VirtualHost *:80>
    ServerAdmin webmaster@localhost
    ServerName <%= fqdn %>
    ServerAlias <%= hostname %>
    ProxyRequests Off
    <Proxy *>
        Order deny,allow
        Allow from all
    </Proxy>
    ProxyPreserveHost on
    ProxyPass /qt-ci/ http://localhost:7181/
    ProxyPass / http://localhost:8080/
</VirtualHost>
