[url "<%= qtgitreadonly %>"]
        insteadof = qtgitreadonly:
[qtqa "hardgit"]
	cachedir = ~/.git_object_cache
        location = <%= location %>
[qtqa "hardgit.server.qtgitreadonly"]
        primary = <%= qtgitreadonly %>
        mirror-<%= location %> = <%= qtgitreadonly_local %>
<%=
    # Within Nokia we've explicitly cached this git repo
    # because it is so huge
    if fqdn =~ /\.nokia\.com$/
        '
[qtqa "hardgit.repo.git://git.webkit.org/WebKit.git"]
        mirror-' + location + ' = ' + qtgitreadonly_local + 'research/webkit-org-webkit-mirror.git
[qtqa "hardgit.repo.git://gitorious.org/webkit/qtwebkit.git"]
        mirror-' + location + ' = ' + qtgitreadonly_local + 'research/gitorious-org-webkit-qtwebkit-mirror.git
'
    else
        ''
    end
%>
[core]
        autocrlf = false
[user]
        name = Qt Continuous Integration System
        email = qt-info@nokia.com
